doi = {10.1016/j.ijheatfluidflow.2015.06.007},
file = {:Users/gsdesena/Library/Application Support/Mendeley Desktop/Downloaded/Wilson, Craft, Iacovides - 2015 - Application of Reynolds stress transport turbulence closure models to flows affected by Lorentz and bu.pdf:pdf},
journal = {International Journal of Heat and Fluid Flow},
keywords = {Magnetic Rayleigh–B{\'{e}}nard convection,Magnetohydrodynamics,Reynolds averaged Navier–Stokes,Second-moment closure,Turbulence closure models},
